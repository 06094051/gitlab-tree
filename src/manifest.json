{
  "name": "gitlab tree",
  "version": "0.0.1",
  "manifest_version": 2,
  "author": "Frank Fan",
  "description": "Chrome extension to display Gitlab code in tree format",
  "homepage_url": "https://github.com/FrankFan/gitlab-tree",
  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": {
    "16": "gitlab.png",
    "48": "gitlab.png",
    "128": "gitlab.png"
  },
  "content_scripts": [
    {
      "matches": ["https://gitlab.com/*", "http://gitlab.lujs.cn/*"],
      "js": ["jquery.js", "jstree.js", "gitlabtree.js"],
      "css": ["main.css", "jstree.css"],
      "run_at": "document_start"
    }
  ]
}